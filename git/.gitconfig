	email = frank.hock@gmail.com

[color]
	ui = auto

[core]
	editor = vim
	pager = less -FRX

[diff]
	algorithm = histogram

[fetch]
	prune = true

[init]
	defaultBranch = main

[merge]
	conflictstyle = diff3

[pull]
	rebase = true

[push]
	default = current

[rerere]
	enabled = true
